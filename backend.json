{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "allegiance": {
        "type": "string"
      },
      "birthday": {
        "type": "string",
        "format": "date"
      },
      "happiness": {
        "type": "integer",
        "minimum": 0,
        "maximum": 10
      },
      "image": {
        "type": "string",
        "format": "uri"
      },
      "name": {
        "type": "string"
      }
    },
    "required": ["allegiance", "birthday", "happiness", "name"],
    "additionalProperties": false
  }
}
